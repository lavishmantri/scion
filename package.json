{
  "name": "obsidian-vault-sync-v2",
  "version": "2.0.0",
  "description": "Sync your vault with custom server via push/pull",
  "main": "main.js",
  "scripts": {
    "dev": "esbuild src/main.ts --bundle --external:obsidian --outfile=main.js --sourcemap --watch",
    "build": "esbuild src/main.ts --bundle --external:obsidian --outfile=main.js",
    "build:prod": "esbuild src/main.ts --bundle --external:obsidian --outfile=main.js --minify"
  },
  "keywords": [
    "obsidian",
    "vault",
    "sync",
    "server"
  ],
  "author": "Lavish Mantri",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "builtin-modules": "3.2.0",
    "esbuild": "0.13.12",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {}
}
